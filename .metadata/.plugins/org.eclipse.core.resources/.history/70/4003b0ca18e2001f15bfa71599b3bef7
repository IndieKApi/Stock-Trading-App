package com.example.demo.entity;

import com.fasterxml.jackson.annotation.JsonAlias;
import lombok.Data;

@Data
public class StockPriceResponse {

    @JsonAlias("symbol")
    private String symbol;

    @JsonAlias("name")
    private String name;

    @JsonAlias("price")
    private Double price;

    @JsonAlias("changesPercentage")
    private Double changesPercentage;

    @JsonAlias("change")
    private Double change;

    @JsonAlias("dayLow")
    private Double dayLow;

    @JsonAlias("dayHigh")
    private Double dayHigh;

    @JsonAlias("yearHigh")
    private Double yearHigh;

    @JsonAlias("yearLow")
    private Double yearLow;

    @JsonAlias("marketCap")
    private Long marketCap;

    @JsonAlias("priceAvg50")
    private Double priceAvg50;

    @JsonAlias("priceAvg200")
    private Double priceAvg200;

    @JsonAlias("exchange")
    private String exchange;

    @JsonAlias("volume")
    private Long volume;

    @JsonAlias("avgVolume")
    private Long avgVolume;

    @JsonAlias("open")
    private Double open;

    @JsonAlias("previousClose")
    private Double previousClose;

    @JsonAlias("eps")
    private Double eps;

    @JsonAlias("pe")
    private Double pe;

    @JsonAlias("earningsAnnouncement")
    private String earningsAnnouncement;

    @JsonAlias("sharesOutstanding")
    private Long sharesOutstanding;

    @JsonAlias("timestamp")
    private Long timestamp;

    @Override
    public String toString() {
        return "StockQuote [symbol=" + symbol + ", name=" + name + ", price=" + price + ", changesPercentage="
                + changesPercentage + ", change=" + change + ", dayLow=" + dayLow + ", dayHigh=" + dayHigh
                + ", yearHigh=" + yearHigh + ", yearLow=" + yearLow + ", marketCap=" + marketCap + ", priceAvg50="
                + priceAvg50 + ", priceAvg200=" + priceAvg200 + ", exchange=" + exchange + ", volume=" + volume
                + ", avgVolume=" + avgVolume + ", open=" + open + ", previousClose=" + previousClose + ", eps=" + eps
                + ", pe=" + pe + ", earningsAnnouncement=" + earningsAnnouncement + ", sharesOutstanding="
                + sharesOutstanding + ", timestamp=" + timestamp + "]";
    }
}
